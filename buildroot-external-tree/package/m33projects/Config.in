config BR2_PACKAGE_M33PROJECTS
	bool "STM32MP2 Firmware examples for M33"
	depends on BR2_TARGET_OPTEE_OS
	help
	  Support for the M33 coprocessor Firmware examples for
	  STM32MP257 platforms. Examples are installed to the
	  /usr/lib/Cube-M33-examples folder.

choice
prompt "Targeted ST Board" if BR2_PACKAGE_M33PROJECTS

config BR2_PACKAGE_M33PROJECTS_MP257F_EV1
	bool "STM32MP257F-EV1"

config BR2_PACKAGE_M33PROJECTS_MP257F_DK
	bool "STM32MP257F-DK"

endchoice
